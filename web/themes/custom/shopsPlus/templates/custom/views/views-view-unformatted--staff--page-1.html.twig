{% if title %}
    <h3>{{ title }}</h3>
    <div class="line-group">
{% endif %}
{% for row in rows %}
    {% set row_classes = [
        default_row_class ? 'views-row',
        ] %}
        <div{{ row.attributes.addClass(row_classes) }}>
            {{ row.content }}
        </div>
    {% endfor %}
{% if title %}
     </div>
{% endif %}